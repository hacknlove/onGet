{
	"env": {
		"es6": true
	},
	"extends": [
		"standard",
    "plugin:jest/recommended"
  ],
	"globals": {
		"Atomics": "readonly",
		"SharedArrayBuffer": "readonly"
	},
	"parserOptions": {
		"ecmaVersion": 2018,
		"sourceType": "module"
  },
  "plugins": [
    "jest"
  ],
	"rules": {
		"no-prototype-builtins": 0,
		"prefer-promise-reject-errors": 0
  }
}
